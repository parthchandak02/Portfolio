{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "An interactive 3D prism visualization built with React and Three.js, featuring glassmorphism UI elements and dynamic light refraction effects",
  "keywords": [
    "react",
    "threejs",
    "3d",
    "prism",
    "visualization",
    "glassmorphism",
    "webgl",
    "interactive",
    "light-refraction"
  ],
  "author": "Parth Chandak",
  "license": "MIT",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/parthchandak02/Portfolio.git"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "homepage": "https://parthchandak.info",
  "private": true,
  "dependencies": {
    "@react-three/drei": "10.0.7",
    "@react-three/fiber": "9.1.2",
    "@react-three/postprocessing": "3.0.4",
    "@types/three": "0.176.0",
    "lucide-react": "^0.525.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-scripts": "5.0.1",
    "react-typed": "^2.0.12",
    "three": "0.176.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.5",
    "kill-port": "^2.0.1",
    "postprocessing": "^6.37.6",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.8",
    "source-map-explorer": "^2.5.3",
    "three-stdlib": "^2.36.0"
  },
  "scripts": {
    "cleanup:port": "node -e \"const {execSync}=require('node:child_process');try{const pids=execSync('lsof -ti tcp:3000',{stdio:['ignore','pipe','ignore']}).toString().trim();if(pids){execSync('kill-port 3000',{stdio:'ignore'});}}catch(_){process.exit(0);}\"",
    "prestart": "npm run cleanup:port",
    "start": "npm run start:app",
    "predev": "npm run cleanup:port",
    "dev": "npm run start:app",
    "start:app": "cross-env GENERATE_SOURCEMAP=false BROWSER=none react-scripts start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css}\"",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "health": "npm outdated && npm audit",
    "outdated": "npm outdated",
    "clean": "rimraf build",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
